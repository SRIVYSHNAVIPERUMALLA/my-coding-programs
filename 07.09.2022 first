#include<stdio.h>
#include<conio.h>
void main()
{
  int f1=0;f2=1,f,n,i,c=0;
  clrscr();
  printf("Enter n value");
  scanf("%d",&n);
  for(i=0;i<=n;i++)
  {
    f=f1+f2;
    if(f==n)
    {
      c++;
      break:
     }
     f1=f2;
     f2=f;
  }
  if(c==1)
  printf("Yes");
  else
  printf("No");
  getch();
 } 
